<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f0f0f0">
    <FrameLayout
        android:id="@+id/group_left"
	    android:layout_width="50dp"
	    android:layout_height="match_parent"
	    android:layout_gravity="left"
	    android:onClick="onBackClick">
        <View
	        android:layout_width="10dp"
	        android:layout_height="20dp"
	        android:background="@drawable/navibar_icon_back"
	        android:layout_gravity="center"/>
    </FrameLayout>
    
    <TextView 
   	    android:id="@+id/textview_title"
   	    android:layout_width="wrap_content"
   		android:layout_height="match_parent" 
   		android:layout_marginLeft="50dp"
   		android:layout_marginRight="50dp"
   		android:textSize="18dp"
   		android:singleLine="true"
   		android:ellipsize="end"
   		android:gravity="center"
   		android:layout_gravity="center"
   		android:textColor="#4e4e4e"/>
    
    <TextView 
   	    android:id="@+id/textview_right_text"
   	    android:layout_width="wrap_content"
   		android:layout_height="match_parent"
   		android:textSize="20dp" 
   		android:singleLine="true"
   		android:ellipsize="end"
   		android:gravity="center"
   		android:layout_gravity="right"
   		android:layout_marginRight="10dp"
   		android:onClick="onRightBtnClick"
   		android:clickable="true"
   		android:textColor="#4e4e4e"/>
    
    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="#b9b9b9"
        android:layout_gravity="bottom"/>
    
    <FrameLayout
        android:id="@+id/ico_addfriend"
	    android:layout_width="50dp"
	    android:layout_height="match_parent"
	    android:layout_gravity="right"
	    android:visibility="gone"
	    android:onClick="onAddfriendClick">
        <View
	        android:layout_width="21dp"
	        android:layout_height="22dp"
	        android:background="@drawable/navibar_addfriend_icon"
	        android:layout_gravity="center"/>
    </FrameLayout>
    
    <FrameLayout
        android:id="@+id/ico_postedit"
	    android:layout_width="60dp"
	    android:layout_height="match_parent"
	    android:visibility="gone"
	    android:onClick="onPostEditClick"
	    android:layout_gravity="right">
        <View
            android:id="@+id/ico"
	        android:layout_width="24dp"
	        android:layout_height="23dp"
	        android:visibility="gone"
	        android:background="@drawable/navibar_icon_delete"
	        android:layout_gravity="center"/>
        <TextView 
	   	    android:id="@+id/textview"
	   	    android:layout_width="wrap_content"
	   		android:layout_height="match_parent"
	   		android:visibility="gone"
	   		android:textSize="17dp"
	   		android:gravity="center"
	   		android:text="@string/edit"
	   		android:layout_gravity="center"
	   		android:textColor="#de504f"/>
    </FrameLayout>
    
    <FrameLayout
        android:id="@+id/ico_share"
	    android:layout_width="60dp"
	    android:layout_height="match_parent"
	    android:visibility="gone"
	    android:onClick="onShareClick"
	    android:layout_gravity="right">
        <View
	        android:layout_width="27dp"
	        android:layout_height="23dp"
	        android:background="@drawable/navibar_icon_share"
	        android:layout_gravity="center"/>
    </FrameLayout>
    
    <FrameLayout
        android:id="@+id/ico_share_more"
	    android:layout_width="60dp"
	    android:layout_height="match_parent"
	    android:visibility="gone"
	    android:onClick="onShareMoreClick"
	    android:layout_gravity="right">
        <View
	        android:layout_width="31dp"
	        android:layout_height="7dp"
	        android:background="@drawable/icon_more_report"
	        android:layout_gravity="center"/>
    </FrameLayout>
    
    <FrameLayout
        android:id="@+id/ico_camera"
	    android:layout_width="60dp"
	    android:layout_height="match_parent"
	    android:visibility="gone"
	    android:onClick="onCameraClick"
	    android:layout_gravity="right">
        <View
	        android:layout_width="32dp"
	        android:layout_height="23dp"
	        android:background="@drawable/navibar_icon_add"
	        android:layout_gravity="center"/>
    </FrameLayout>
    
    <FrameLayout
        android:id="@+id/ico_webclose"
	    android:layout_width="60dp"
	    android:layout_height="match_parent"
	    android:visibility="gone"
	    android:onClick="onWebCloseClick"
	    android:layout_gravity="right">
        <View
	        android:layout_width="18dp"
	        android:layout_height="18dp"
	        android:background="@drawable/icon_delete"
	        android:layout_gravity="center"/>
    </FrameLayout>
    
    <LinearLayout
        android:id="@+id/group_kcb"
	    android:layout_width="wrap_content"
	    android:layout_height="match_parent"
	    android:onClick="onKcbWeekClick"
	    android:visibility="gone"
	    android:layout_marginRight="-10dp"
	    android:layout_gravity="center"
	    android:orientation="horizontal">
        <TextView
            android:id="@+id/textview_kcbweektop"
	   	    android:layout_width="wrap_content"
	   		android:layout_height="match_parent"
	   		android:layout_marginRight="5dp"
	   		android:textSize="20dp"
	   		android:textColor="#4c4c4c"
	   		android:text="@string/kcb_details_zs"
	   		android:gravity="center"/>
        <View 
            android:id="@+id/view_arrow"
	   	    android:layout_width="17dp"
	   		android:layout_height="9dp"
	   		android:layout_gravity="center"
	   		android:background="@drawable/navbar_pulldown_arrow"/>
    </LinearLayout>
    
    <TextView 
   	    android:id="@+id/textview_forgetpass"
   	    android:layout_width="wrap_content"
   		android:layout_height="match_parent" 
   		android:layout_marginRight="10dp"
   		android:visibility="gone"
   		android:clickable="true"
   		android:onClick="onTipBtnClick"
   		android:textSize="15dp"
   		android:gravity="center"
   		android:text="@string/login_forgetpass"
   		android:layout_gravity="right"
   		android:textColor="#de504f"/>
    
    <TextView 
   	    android:id="@+id/textview_done"
   	    android:layout_width="wrap_content"
   		android:layout_height="match_parent" 
   		android:layout_marginRight="10dp"
   		android:visibility="gone"
   		android:clickable="true"
   		android:onClick="onTipBtnClick"
   		android:textSize="16dp"
   		android:gravity="center"
   		android:text="@string/done"
   		android:layout_gravity="right"
   		android:textColor="#de504f"/>
    
    <TextView 
   	    android:id="@+id/textview_publish"
   	    android:layout_width="wrap_content"
   		android:layout_height="match_parent" 
   		android:layout_marginRight="10dp"
   		android:visibility="gone"
   		android:clickable="true"
   		android:onClick="onPublishClick"
   		android:textSize="16dp"
   		android:gravity="center"
   		android:text="@string/publish"
   		android:layout_gravity="right"
   		android:textColor="#de504f"/>
    
    <TextView 
   	    android:id="@+id/textview_jfsm"
   	    android:layout_width="wrap_content"
   		android:layout_height="match_parent" 
   		android:layout_marginRight="10dp"
   		android:visibility="gone"
   		android:clickable="true"
   		android:onClick="onJfsmClick"
   		android:textSize="16dp"
   		android:gravity="center"
   		android:text="@string/myintegral_web"
   		android:layout_gravity="right"
   		android:textColor="#3b3f42"/>
    
    <TextView 
   	    android:id="@+id/textview_checkvote"
   	    android:layout_width="wrap_content"
   		android:layout_height="match_parent" 
   		android:layout_marginRight="10dp"
   		android:visibility="gone"
   		android:clickable="true"
   		android:onClick="onVoteResultClick"
   		android:textSize="16dp"
   		android:gravity="center"
   		android:text="@string/vote_checkresult"
   		android:layout_gravity="right"
   		android:textColor="#de504f"/>

</FrameLayout>
