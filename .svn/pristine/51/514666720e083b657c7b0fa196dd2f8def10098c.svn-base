<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F3F3F3" >
    <ScrollView 
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <RelativeLayout 
			    android:layout_width="match_parent"
			    android:layout_height="wrap_content"
			    android:background="#F3F3F3" >
			   <LinearLayout 
				    android:id="@+id/store_order_layout"
				    android:layout_width="match_parent"
				    android:layout_height="90dp"
				    android:orientation="vertical"
				    android:gravity="center_vertical"
				    android:background="#ffffff"
				    android:layout_alignParentTop="true"> 
			    <RelativeLayout 
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="14dp"
			        android:layout_marginRight="14dp">
			        <TextView            
			            android:id="@+id/store_consignee"
			            android:layout_width="wrap_content"
			            android:layout_height="wrap_content"
			            android:textColor="#000000"
			            android:textSize="16dp"
			            android:layout_centerVertical="true"
			            android:gravity="center"/>
			         <TextView	             
			           	android:id="@+id/store_consignee_phone"
			            android:layout_width="wrap_content"
			            android:layout_height="wrap_content"
			            android:textColor="#000000"
			            android:textSize="16dp"
			           	android:layout_alignParentRight="true"
			           	android:layout_centerVertical="true"/>                
			    </RelativeLayout>
				<RelativeLayout 
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:layout_marginLeft="14dp"
			        android:layout_marginRight="14dp"
			        android:layout_marginTop="20dp"
			        android:layout_gravity="center_vertical">
			        <ImageView
			            android:layout_width="16dp"
			            android:layout_height="16dp"
			            android:src="@drawable/icon_location"
			            android:layout_centerVertical="true"
			            android:gravity="center"
			            android:id="@+id/icon_location"
			            android:background="#000000"/>
			         <TextView	             
			           	android:id="@+id/store_address"
			            android:layout_width="wrap_content"
			            android:layout_height="wrap_content"
			            android:textColor="#000000"
			            android:textSize="16dp"
			           	android:layout_centerVertical="true"
			           	android:layout_toRightOf="@id/icon_location"
			           	android:layout_marginLeft="14dp"/>                
			    </RelativeLayout>
			</LinearLayout>
			
			<View
			    android:id="@+id/store_order_lines"
			    android:layout_width="match_parent"
			    android:layout_height="1dp"
			    android:background="#F3F3F3"
			    android:layout_below="@id/store_order_layout"/>
			
			<RelativeLayout 
			    android:id="@+id/store_listitem_layout"
			       android:layout_width="match_parent"
			       android:layout_height="40dp"
			       android:orientation="horizontal"
			       android:background="#FFFFFF"
			       android:layout_below="@id/store_order_lines">
			        <ImageView 
				    android:layout_width="20dp"
				    android:layout_height="20dp"
				    android:src="@drawable/nav_icon_homepage"
				    android:layout_centerVertical="true"
				    android:layout_alignParentLeft="true"
				    android:layout_marginLeft="14dp"
				    android:id="@+id/home"/>
				 <TextView
	               	android:layout_width="wrap_content"
	               	android:layout_height="wrap_content"
	               	android:id="@+id/store_name"
	               	android:layout_centerVertical="true"
	               	android:layout_marginLeft="15dp"
	               	android:textColor="#000000"
	               	android:gravity="center_vertical"
	              	android:layout_toRightOf="@id/home" />
				  <ImageView 
				    android:layout_width="20dp"
				    android:layout_height="20dp"
				    android:src="@drawable/arrow"
				    android:layout_centerVertical="true"
				    android:layout_marginLeft="14dp"
				    android:layout_toRightOf="@id/store_name"/>  		    
			   </RelativeLayout>
			   
			   <View
			       android:id="@+id/store_check_order_line"        
			       android:layout_width="match_parent"
			       android:layout_height="1dp"
			       android:background="#E8E8E8"
			       android:layout_below="@id/store_listitem_layout"/>
			   
			   <View
			       android:id="@+id/store_check_order_lines"
			       android:layout_width="match_parent"
			       android:layout_height="1dp"
			       android:background="#F5F5F5"
			       android:layout_below="@id/store_check_order_line"/>
			   
			   <com.layout.ListViewForScrollView 
			       android:id="@+id/order_goods_list"
			       android:layout_width="match_parent"
			       android:layout_height="wrap_content"
			       android:divider="#F3F3F3"
			       android:dividerHeight="1dp"
			       android:listSelector="@android:color/transparent"
			       android:layout_below="@id/store_check_order_line"/>
			   
			   <RelativeLayout 
			       android:id="@+id/order_freight"
			       android:layout_width="match_parent"
			       android:layout_height="40dp"
			       android:background="#FFFFFF"
			       android:layout_below="@id/order_goods_list">
			       <TextView 
				     android:layout_width="wrap_content"
				     android:layout_height="wrap_content"
				     android:text="@string/store_freight"
				     android:layout_gravity="center_vertical"
				     android:textColor="#000000"
				     android:textSize="13dp"
				     android:layout_centerVertical="true"
				     android:layout_marginLeft="14dp"/>
				 <TextView 
				     android:id="@+id/store_my_order_sum"
				     android:layout_width="wrap_content"
				     android:layout_height="wrap_content"
				     android:layout_gravity="center_vertical"
				     android:textColor="#DD514F"
				     android:textSize="13dp"
				     android:layout_centerVertical="true"
				     android:layout_alignParentRight="true"
				     android:layout_marginRight="14dp"/>
			   </RelativeLayout>
			   
			   <View
			       android:id="@+id/line_one" 
			       android:layout_width="match_parent"
			       android:layout_height="1dp"
			       android:background="#E8E8E8"
			       android:layout_below="@id/order_freight"/>
			   
			   <RelativeLayout
			       android:id="@+id/store_send_goods"
			       android:layout_width="match_parent"
			       android:layout_height="40dp"
			       android:clickable="true"
			       android:focusable="true"
			       android:focusableInTouchMode="true"
			       android:background="#FFFFFF"
			       android:layout_below="@id/line_one">
			       <TextView 
				     android:layout_width="wrap_content"
				     android:layout_height="wrap_content"
				     android:text="@string/store_shopping_method"
				     android:layout_gravity="center_vertical"
				     android:textColor="#000000"
				     android:textSize="13dp"
				     android:layout_centerVertical="true"
				     android:layout_marginLeft="14dp"
				     android:clickable="false"
			         android:focusable="false"
			         android:focusableInTouchMode="false"/>
				 <ImageView 
				     android:layout_width="13dp"
				     android:layout_height="13dp"
				     android:src="@drawable/arrow"
				     android:layout_alignParentRight="true"
				     android:layout_centerVertical="true"
				     android:id="@+id/arrow"
				     android:layout_marginRight="14dp"
				     android:clickable="false"
			         android:focusable="false"
			         android:focusableInTouchMode="false"/>
				 <TextView
				     android:id="@+id/store_shopping_delivery" 
				     android:layout_width="wrap_content"
				     android:layout_height="wrap_content"
				     android:text="@string/store_shopping_delivery"
				     android:layout_gravity="center_vertical"
				     android:textColor="#000000"
				     android:textSize="13dp"
				     android:layout_centerVertical="true"
				     android:layout_toLeftOf="@id/arrow"
				     android:clickable="false"
			         android:focusable="false"
			         android:focusableInTouchMode="false"/>
			   </RelativeLayout>
			   
			   <View 
			       android:id="@+id/store_line_two"
			       android:layout_width="match_parent"
			       android:layout_height="1dp"
			       android:background="#E8E8E8"
			       android:layout_below="@id/store_send_goods"/>
			   	
			   <LinearLayout 
			       android:id="@+id/store_order_devlive_layout"
			       android:layout_width="match_parent"
			       android:layout_height="40dp"
			       android:orientation="horizontal"
			       android:gravity="right"
			       android:background="#FFFFFF"
			       android:layout_below="@id/store_line_two">
				 <TextView 
				     android:id="@+id/store_my_order_count"		     
				     android:layout_width="wrap_content"
				     android:layout_height="wrap_content"
				     android:layout_gravity="center_vertical"
				     android:textColor="#000000"
				     android:textSize="13dp"
				     android:layout_marginRight="19dp"/>
				 <TextView 
				     android:layout_width="wrap_content"
				     android:layout_height="wrap_content"
				     android:text="@string/store_total"
				     android:layout_gravity="center_vertical"
				     android:textColor="#000000"
				     android:textSize="13dp"     
				     android:layout_marginRight="19dp"/>
				 <TextView 
				     android:id="@+id/store_my_order_count_sum"
				     android:layout_width="wrap_content"
				     android:layout_height="wrap_content"
				     android:layout_gravity="center_vertical"
				     android:textColor="#DD514F"
				     android:textSize="13dp"
				     android:layout_marginRight="14dp"/>
			   </LinearLayout>
			   
			   <View
			       android:id="@+id/store_line_three"
			       android:layout_width="match_parent"
			       android:layout_height="5dp"
			       android:background="#F3F3F3"
			       android:layout_below="@id/store_order_devlive_layout"/>
			   
			   <LinearLayout 
			       android:layout_width="match_parent"
			       android:layout_height="100dp"
			       android:background="#FFFFFF"
			       android:layout_below="@id/store_line_three"
			       android:orientation="vertical">
				 <TextView
				     android:id="@+id/order_number"
				     android:layout_width="match_parent"
				     android:layout_height="wrap_content"
				     android:textSize="11dp"
				     android:textColor="@color/gray"
				     android:layout_marginLeft="10dp"
				     android:layout_marginTop="5dp"/>
				 <TextView
				     android:id="@+id/order_create_time"
				     android:layout_width="match_parent"
				     android:layout_height="wrap_content"
				     android:textSize="11dp"
				     android:textColor="@color/gray"
				     android:layout_marginLeft="10dp"
				     android:layout_marginTop="5dp"/>
				 <TextView
				     android:id="@+id/order_pay_time"
				     android:layout_width="match_parent"
				     android:layout_height="wrap_content"
				     android:textSize="11dp"
				     android:textColor="@color/gray"
				     android:layout_marginLeft="10dp"
				     android:layout_marginTop="5dp"/>
				 <TextView
				     android:id="@+id/order_send_time"
				     android:layout_width="match_parent"
				     android:layout_height="wrap_content"
				     android:textSize="11dp"
				     android:textColor="@color/gray"
				     android:layout_marginLeft="10dp"
				     android:layout_marginTop="5dp"/>
				 <TextView
				     android:id="@+id/order_success_time"
				     android:layout_width="match_parent"
				     android:layout_height="wrap_content"
				     android:textSize="11dp"
				     android:textColor="@color/gray"
				     android:layout_marginLeft="10dp"
				     android:layout_marginTop="5dp"/>				 
			</LinearLayout>
		</RelativeLayout>
	</ScrollView>
    <FrameLayout 
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#FFFFFF"
        android:layout_alignParentBottom="true">        
        <TextView 
		     android:id="@+id/store_my_order_confirm"
		     android:layout_width="wrap_content"
		     android:layout_height="wrap_content"
		     android:layout_gravity="center_vertical|right"
		     android:textColor="#DD514F"
		     android:textSize="14dp"
		     android:background="@drawable/shape_store_car_bg"
		     android:gravity="center"
		     android:layout_marginRight="10dp" />
    </FrameLayout>
 
</RelativeLayout>
