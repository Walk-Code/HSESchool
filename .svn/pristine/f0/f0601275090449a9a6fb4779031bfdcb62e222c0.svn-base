<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    
    <RelativeLayout
        android:id="@+id/group_bottom"
	    android:layout_width="match_parent"
	    android:layout_height="65dp"
	    android:visibility="gone"
	    android:layout_alignParentBottom="true">
        <TextView 
	   	    android:id="@+id/textview_commit"
	   	    android:layout_width="match_parent"
	   		android:layout_height="40dp" 
	   		android:layout_marginLeft="10dp"
	   		android:layout_marginRight="10dp"
	   		android:layout_centerInParent="true"
	   		android:textSize="17dp"
	   		android:textColor="#ffffff"
	   		android:text="@string/vote_commit"
	   		android:background="@drawable/btn_red_button_selector"
	   		android:clickable="true"
	   		android:onClick="onCommitBtnClick"
	   		android:gravity="center"/>
    </RelativeLayout>
    
    <ListView
	    android:id="@+id/listview"
	    android:layout_above="@id/group_bottom"
	    android:layout_width="match_parent"
	    android:layout_height="match_parent"
	    android:divider="@null"
	    android:listSelector="#00000000"
	    android:cacheColorHint="#00000000"
	    android:fadingEdge="none" />

</RelativeLayout>
